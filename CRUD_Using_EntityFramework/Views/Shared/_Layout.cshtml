<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    <link href="~/Content/Site.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <script src="~/Scripts/modernizr-2.6.2.js"></script>
</head>
<body>

    <nav class="navbar navbar-inverse">
        <div class="container-fluid">
            <div class="navbar-header">
                @* <a class="navbar-brand" href="#">WebSiteName</a>*@
                @Html.ActionLink("GAnesh pawar", "Index", "Trainers", null, new { @class = "navbar-brand" })
            </div>
            <ul class="nav navbar-nav">
                <li class="active">@Html.ActionLink("TRiners", "Index", "Trainers")</li>
                <li>@Html.ActionLink("STudents", "Index", "students")</li>

            </ul>
            </ul>
            <ul class="nav navbar-nav navbar-right">
                <li><a href="Account/Register"><span class="glyphicon glyphicon-user"></span> Sign Up</a></li>
                <li><a href="Account/Login"><span class="glyphicon glyphicon-log-in"></span> Login</a></li>
            </ul>
        </div>
    </nav>






    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>

    <script src="~/Scripts/jquery-1.10.2.min.js"></script>
    <script src="~/Scripts/bootstrap.min.js"></script>
</body>
</html>